<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="40dp"
        android:height="40dp">
        <shape android:shape="oval">
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item
        android:width="36dp"
        android:height="36dp"
        android:top="2dp"
        android:bottom="2dp"
        android:end="2dp"
        android:start="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:drawable="@drawable/image_edit_icon"
        android:width="30dp"
        android:height="30dp"
        android:top="5dp"
        android:start="6dp" />
</layer-list>